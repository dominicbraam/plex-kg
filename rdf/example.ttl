# using schema.org since they have relevant schemas for this project
@prefix schema: <https://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@base <http://plex-kg/> .

<genre/action> schema:name "Action"@en .

<genre/adventure> schema:name "Adventure"@en .

<genre/science-fiction> schema:name "Science Fiction"@en .

<person/plex-watcher> a schema:Person ;
    schema:name "Plex Watcher"@en .

<person/ChristopherNolan> a schema:Person ;
    schema:name "Christopher Nolan"@en .

<movie/iron-man-2> a schema:Movie ;
    schema:name "Iron Man 2"@en ;
    schema:genre <genre/action>,
        <genre/adventure> ;
    schema:duration "PT2H4M"^^xsd:duration ;
    schema:director <person/jon-favreau> ;
    schema:datePublished "2010-04-28"^^xsd:date ;
    schema:aggregateRating [ a schema:AggregateRating ;
            schema:ratingValue 7.2 ] ;
    schema:author <person/don-heck>,
        <person/justin-theroux> ;
    schema:actor <person/don-cheadle>,
        <person/gwyneth-paltrow>,
        <person/robert-downey-jr> .

<tvseries/GOT> a schema:TVSeries;
    schema:name "Game of Thrones"@en;
    schema:genre <genre/science-fiction>.

<tvseries/GOT/ep1> a schema:TVEpisode;
    schema:partOfSeries <tvseries/GOT>;
    schema:name "Episode 1"@en;
    schema:director <person/plex-watcher>;
    schema:datePublished "2025-01-01"^^xsd:date.

<history/status/sessions/history/1058> a schema:WatchAction ;
    schema:agent <person/plex-watcher> ;
    schema:object <movie/iron-man-2> ;
    schema:startTime "2022-12-25"^^xsd:date .
